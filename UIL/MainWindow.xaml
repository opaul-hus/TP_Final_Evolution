<Window x:Class="UIL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:GenreEnum="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:UIL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid Name="gPrincipal">

        <Grid.ColumnDefinitions>
            <ColumnDefinition ></ColumnDefinition>
            
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="20*"></RowDefinition>
        </Grid.RowDefinitions>
        <Menu>
            <MenuItem Header="Fichier">
                <MenuItem Header="Données d’immatriculation" Click="Donnes_Click"/>
                <MenuItem Header="Immatriculations par province"/>
                <MenuItem Header="Variation d’immatriculation"/>
                <Separator/>
                <MenuItem Header="Quitter" Click="Quitter_Click"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
